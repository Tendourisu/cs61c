window.initDarkToggle=function(){const c="cs61cDarkMode",l="darkMode";let d=null,r=null,o=null,t=!1,a=!1;function s(e,n){n!==void 0&&(a=n),(!a||n!==void 0)&&(t=e),a?(t?(document.documentElement.classList.add("theme-dark"),document.documentElement.classList.remove("theme-light")):(document.documentElement.classList.add("theme-light"),document.documentElement.classList.remove("theme-dark")),o&&o.classList.remove("d-none")):(document.documentElement.classList.remove("theme-light"),document.documentElement.classList.remove("theme-dark"),o&&o.classList.add("d-none")),r&&r.checked!==t&&(r.checked=t);try{a?window.localStorage.setItem(c,String(t)):window.localStorage.removeItem(c)}catch(i){console.error(i)}}try{let e=window.localStorage.getItem(c);if(typeof e!="string"){const n=window.localStorage.getItem(l);typeof n=="string"&&(e=n,window.localStorage.setItem(c,n),window.localStorage.removeItem(l))}typeof e=="string"&&(a=!0,e==="true"&&(t=!0))}catch(e){console.error(e)}try{const e=window.matchMedia("(prefers-color-scheme: dark)");a||(t=e.matches),e.addEventListener("change",n=>{s(n.matches)})}catch(e){console.error(e)}s(t,a),document.addEventListener("DOMContentLoaded",()=>{d=document.getElementById("dark-toggle-wrapper"),r=document.getElementById("inputDarkToggle"),o=document.getElementById("buttonDarkToggleReset"),s(t,a),r.addEventListener("change",e=>{s(e.target.checked,!0)}),o.addEventListener("click",e=>{e.preventDefault();let n=!1;try{n=window.matchMedia("(prefers-color-scheme: dark)").matches}catch(i){console.error(i)}s(n,!1)}),d.classList.remove("d-none")})},window.check404References=function(){const c=document.getElementById("404-references");if(!c)return;let l=window.location.pathname;if(l[0]==="/"&&(l=l.slice(1)),!l)return;const d="sp22",r=l.split("/");if(r[0].length>=1){const o=r[0].match(/^((sp|su|fa)(\d{2,}))$/);if(o&&(!d||o!==d)){c.appendChild(document.createTextNode("Possible archive link: "));const t=document.createElement("a");t.href=`https://inst.eecs.berkeley.edu/~cs61c/${o[1]}/${r.slice(1).join("/")}`,t.innerText=t.href,c.appendChild(t),window.location.href=t.href}}};
